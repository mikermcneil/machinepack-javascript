  },
  "keywords": [
    "javascript",
    "minify",
    "machine",
    "machines"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "test-machinepack-mocha": "^2.1.1"
  },
  "repository": {
    "url": "git://github.com/mikermcneil/machinepack-javascript.git"
  },
  "browserify": {
    "transform": [
      "browserify-transform-machinepack"
    ]
  },
  "dependencies": {
    "browserify-transform-machinepack": "^1.0.3",
    "convert-to-ecmascript-compatible-varname": "^0.1.2",
    "js-beautify": "^1.5.5",
    "machine": "^10.3.1",
    "uglify-js": "^2.4.15"
